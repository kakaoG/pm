<div class="coupon <#=data.status#>">
    <div class="coupon-main">
        <div class="title"><#=data.title#></div>
        <div class="sub-title condition">&nbsp;<#=data.subTitle#>&nbsp;</div>
        <div class="expiry-date">有效期 <#=data.region#></div>
    </div>
    <div class="coupon-action">
        <div class="name"><#=data.ownerName#></div>
        <div class="scan" data-code="<#=data.code#>"><img src="/img/m/index/mall_code.jpg" alt=""/>扫一扫</div>
        <div class="num"><span>编号:</span><#=data.code#></div>
    </div>
</div>