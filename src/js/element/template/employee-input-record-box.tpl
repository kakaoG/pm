<div class="employee-input-record-box g-hrz <#=data.fieldName#>">
	<div class="text u-wrap"><#=data.text#></div>
	<div class="detail u-wrap">
		<div class="left">
			<div class="pie pie-left">
				<div class="pie-label">
					<#=data.data.left.label#>
				</div>
				<div class="pie-text">
					<#=data.data.left.text#>
				</div>	
			</div>		
		</div>	
		<div class="right">
			<div class="pie pie-right">
				<div class="pie-label">
					<#=data.data.right.label#>
				</div>
				<div class="pie-text">
					<#=data.data.right.text#>
				</div>	
			</div>		
		</div>	
		<div class="clear-both"></div>
	</div>
</div>