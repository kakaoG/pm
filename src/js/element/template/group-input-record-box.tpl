<div class="group-input-record-box <#=data.fieldName#>">
	<div class="item col1 input-member-amount" >
		<div class="label">已录住宅</div>
		<div class="value"><#=data.input_member_amount#></div>
	</div>
	<div class="item  col2 uninput-member-amount" >
		<div class="label">未录住宅</div>
		<div class="value"><#=data.uninput_member_amount#></div>
	</div>
	<div class="item  col2 input-member-rate" >
		<div class="label">已录比率</div>
		<div class="value"><#=data.input_member_rate#>%</div>
	</div>
	<div class="item col2 input-community-amount" >
		<div class="label">已录小区</div>
		<div class="value"><#=data.input_community_amount#></div>
	</div>
	<div class="item col2 employee-count" >
		<div class="label">全国员工</div>
		<div class="value"><#=data.employee_count#></div>
	</div>
	<div class="clear-both"></div>
</div>